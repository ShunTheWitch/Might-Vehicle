<?xml version="1.0" encoding="utf-8" ?>

<Defs>	
	<Vehicles.VehicleDef ParentName="VehiclePawn_Armored">
		<defName>CVN_Mightboomer</defName>
		<label>Might</label>
		<description>The Might Amphibious Walker was made materials from blueprints lost from old earth unfortunately for all life, mechanoids found these blueprints and interprited them by their own designs, causing a drastic shift from an ICBM platform to a mobile carrier that alone with it's four SSM Judgement launchers and fabricators, Two charge autocannons, and heavy AA cannon has been seen to sweep across entire urbworlds unopposed</description>
		
		<graphicData>
			<texPath>Things/Vehicles/Tier2/ZTZ99A/Tier2_Tank</texPath>
			<graphicClass>Vehicles.Graphic_Vehicle</graphicClass>
			<shaderType>CutoutComplexPattern</shaderType>
			<drawSize>(5, 11)</drawSize>
			<color>(155, 155, 155)</color>
			<colorTwo>(65, 75, 80)</colorTwo>
			<colorThree>(45, 50, 45)</colorThree>
		</graphicData> 
		
		<fillPercent>1</fillPercent>
		
		<buildDef>CVN_Mightboomer_blueprint</buildDef>
		
		<size>(4, 10)</size>
		
		<vehicleMovementPermissions>DriverNeeded</vehicleMovementPermissions>
		<vehicleCategory>Combat</vehicleCategory>
		<techLevel>Industrial</techLevel>
		<vehicleType>Land</vehicleType>
		
		<statBases>
			<ArmorRating_Blunt>1.48</ArmorRating_Blunt>
			<ArmorRating_Sharp>1.85</ArmorRating_Sharp>
			<Flammability>0.5</Flammability>
      <EnergyShieldRechargeRate>0.5</EnergyShieldRechargeRate>
      <EnergyShieldEnergyMax>3</EnergyShieldEnergyMax>
			<MarketValue>2240</MarketValue>
		</statBases>
		
		<vehicleStats>
			<Mass>450</Mass>
			<MoveSpeed>10.2</MoveSpeed>
			<CargoCapacity>1000</CargoCapacity>
			<RepairRate>4</RepairRate>
		</vehicleStats>
		
		<soundSustainersOnEvent>
			<li>
				<key>(IgnitionOn, IgnitionOff)</key>
				<value>VVE_VehicleEngineHeavy</value>
			</li>
		</soundSustainersOnEvent>
		
		<race>
			<baseBodySize>10.5</baseBodySize>
		</race>
		
		<properties>
			<customRoadCosts AssignDefaults = "0.4" />
			<vehicleJobLimitations>
				<li>(RepairVehicle, 20)</li>
			</vehicleJobLimitations>
			<damageDefMultipliers>
    <li>
         <key>MechBandShockwave</key>
         <value>0</value>
    </li>
</damageDefMultipliers>
			<roles>
				<li>
					<key>passenger</key>
					<label>Passenger</label>
					<slots>20</slots>
				</li>
			</roles>
		</properties>
		
		<components>
			<li>
				<key>Engine</key>
				<label>Fusion ReactorEngine</label>
				<health>300</health>
				<priorityStatEfficiency>true</priorityStatEfficiency>
				<depth>External</depth>
				
				<reactors>
					<li Class = "Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>1.0</chance>
						<maxHealth>1</maxHealth>
						<radius>200</radius>
						<damageDef>MechBandShockwave</damageDef>
					</li>
				</reactors>
				
				<hitbox>
					<cells>
						<li>(0, -1)</li>
						<li>(0, -2)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>LeftTrack</key>
				<label>Left Track</label>
				<health>180</health>
				<depth>External</depth>
				
				<tags>
					<li>Wheel</li>
				</tags>
				
				<hitbox>
					<fallthrough>false</fallthrough>
					<side>Left</side>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
